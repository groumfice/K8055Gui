        <TextBlock x:ConnectionId='34' x:Name="textBlock5" HorizontalAlignment="Left" Margin="325,220,0,0" TextWrapping="Wrap" Text="Windows 10 IOT" VerticalAlignment="Top" FontWeight="Bold" Foreground="#FF4570E4" FontSize="14"/>
