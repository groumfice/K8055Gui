        <Button x:ConnectionId='16' x:Name="btOutput8" HorizontalAlignment="Left" Margin="739,148,0,0" VerticalAlignment="Top" Width="24" Height="24" Background="Gray" Content="8" FontSize="10" IsEnabled="False"/>
