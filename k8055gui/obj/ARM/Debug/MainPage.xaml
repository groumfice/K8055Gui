        <ComboBox x:ConnectionId='29' x:Name="cbAdresse" HorizontalAlignment="Left" Margin="181,46,0,0" VerticalAlignment="Top" Width="68">
            <ComboBoxItem Content="0" IsSelected="True"/>
            <ComboBoxItem Content="1"/>
            <ComboBoxItem Content="2"/>
            <ComboBoxItem Content="3"/>
        </ComboBox>
        <TextBlock x:ConnectionId='30' x:Name="textBlock1" HorizontalAlignment="Left" Margin="113,52,0,0" TextWrapping="Wrap" Text="Address :" VerticalAlignment="Top" Height="31" Width="82" RenderTransformOrigin="0.5,0.5"/>
        <Button x:ConnectionId='31' x:Name="btQuit" Content="Quitter" HorizontalAlignment="Left" Margin="30,84,0,0" VerticalAlignment="Top" Width="75"/>
        <TextBlock x:ConnectionId='32' x:Name="textBlock2" HorizontalAlignment="Left" Margin="250,188,0,0" TextWrapping="Wrap" Text="RaspK8055" VerticalAlignment="Top" Height="54" Width="212" FontSize="36" FontFamily="Segoe UI Black" FontWeight="Bold"/>
