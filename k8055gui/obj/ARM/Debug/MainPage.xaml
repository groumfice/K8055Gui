        <Button x:ConnectionId='3' x:Name="btconnect" Content="Connect" HorizontalAlignment="Left" Margin="30,46,0,0" VerticalAlignment="Top" IsDoubleTapEnabled="False" IsHoldingEnabled="False" IsRightTapEnabled="False" IsTapEnabled="False"/>
        <Button x:ConnectionId='4' x:Name="btInput5" HorizontalAlignment="Left" Margin="30,402,0,0" VerticalAlignment="Top" Width="24" Height="24" Background="Gray" Content="5" FontSize="10"/>
        <Button x:ConnectionId='5' x:Name="btInput4" Content="4" HorizontalAlignment="Left" Margin="30,356,0,0" VerticalAlignment="Top" Width="24" Height="24" Background="Gray" FontSize="10"/>
        <Button x:ConnectionId='6' x:Name="btInput3" HorizontalAlignment="Left" Margin="30,310,0,0" VerticalAlignment="Top" Width="24" Height="24" Background="Gray" Content="3" FontSize="10"/>
        <Button x:ConnectionId='7' x:Name="btInput2" HorizontalAlignment="Left" Margin="30,264,0,0" VerticalAlignment="Top" Width="24" Height="24" Background="Gray" Content="2" FontSize="10"/>
        <Button x:ConnectionId='8' x:Name="btInput1" HorizontalAlignment="Left" Margin="30,218,0,0" VerticalAlignment="Top" Width="24" Height="24" Background="Gray" Content="1" FontSize="10"/>
        <Button x:ConnectionId='9' x:Name="btOutput1" HorizontalAlignment="Left" Margin="739,414,0,0" VerticalAlignment="Top" Width="24" Height="24" Background="Gray" Content="1" FontSize="10" IsEnabled="False"/>
        <Button x:ConnectionId='10' x:Name="btOutput2" Content="2" HorizontalAlignment="Left" Margin="739,376,0,0" VerticalAlignment="Top" Width="24" Height="24" Background="Gray" FontSize="10" IsEnabled="False"/>
        <Button x:ConnectionId='11' x:Name="btOutput3" HorizontalAlignment="Left" Margin="739,338,0,0" VerticalAlignment="Top" Width="24" Height="24" Background="Gray" Content="3" FontSize="10" IsEnabled="False"/>
        <Button x:ConnectionId='12' x:Name="btOutput4" HorizontalAlignment="Left" Margin="739,300,0,0" VerticalAlignment="Top" Width="24" Height="24" Background="Gray" Content="4" FontSize="10" IsEnabled="False"/>
        <Button x:ConnectionId='13' x:Name="btOutput5" HorizontalAlignment="Left" Margin="739,262,0,0" VerticalAlignment="Top" Width="24" Height="24" Background="Gray" FontSize="10" Content="5" IsEnabled="False"/>
